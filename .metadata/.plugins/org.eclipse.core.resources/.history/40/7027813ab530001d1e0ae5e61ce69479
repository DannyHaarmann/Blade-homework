
public class Salesmen 
{
	private String Name;
	public int Sales;
	
	public Salesmen(String Name, int Sales)
	{
		this.Name = Name;
		this.Sales = Sales;
	}
	
	public static Salesmen[] TopFive(Salesmen[] sales)
	{
		Salesmen top5[] = new Salesmen[5];
		
		for(int i = 0; i < 5; i++)
		{
			if(top5[i] == null)
			{
				System.out.println("it is null");
			}
		}
		return top5;
	}
}
