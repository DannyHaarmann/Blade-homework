

	



public class Driver 
{
	public static void main(String[] args)
	{
		
		int arr[] = {  3,65,6565,234,78,6};
		int[] Salesmen = new int[5];
		
		

		
		
		bubbleSort(arr);
		System.out.println("Salesmen complete");
		for(int i = 0; i < 5;i++)
		{
			System.out.println(arr[i] + " ");
		}
	}
	public static void bubbleSort( int[] arr) 
	 {  
	        int n = arr.length;  
	        int temp = 0;  
	         for(int i=0; i < n; i++)
	         {  
	                 for(int j=1; j < (n-i); j++)
	                 {  
	                          if(arr[j-1] < arr[j])
	                          {  
	                                 
	                                 temp = arr[j-1];  
	                                 arr[j-1] = arr[j];  
	                                 arr[j] = temp;  
	                         }  
	                          
	                 }  
	         }  
	 }	
	

		
}
